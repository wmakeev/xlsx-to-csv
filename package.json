{
  "name": "@wmakeev/xlsx-to-csv",
  "version": "0.0.2",
  "type": "module",
  "description": "Streaming convert from xlsx to csv with fine tuning based on exceljs lib.",
  "keywords": [
    "xlsx",
    "csv",
    "convert",
    "exceljs"
  ],
  "exports": "./build/src/index.js",
  "files": [
    "build/src/"
  ],
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "test": "node -r dotenv/config ./build/test/index.test.js",
    "lint": "eslint . --fix",
    "cleanup": "rm -rf build/",
    "compile:dev": "tsc",
    "compile": "tsc --build tsconfig.prod.json",
    "build:dev": "npm run lint && npm run compile:dev",
    "build": "npm run cleanup && npm run lint && npm run compile",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "release": "np"
  },
  "engines": {
    "node": ">=18"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "np": {
    "yarn": false
  },
  "author": "Vitaliy V. Makeev",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmakeev/xlsx-to-csv"
  },
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@tsconfig/strictest": "^2.0.2",
    "@types/highland": "^2.12.20",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.18.13",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@typescript-eslint/typescript-estree": "^6.10.0",
    "auto-changelog": "^2.4.0",
    "csv-stringify": "^6.4.4",
    "dotenv": "^16.3.1",
    "envalid": "^8.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "lodash-es": "^4.17.21",
    "np": "^9.0.0",
    "prettier": "^3.1.0",
    "typescript-eslint-language-service": "^5.0.5"
  },
  "dependencies": {
    "@wmakeev/highland-tools": "^1.9.1",
    "exceljs": "^4.4.0",
    "highland": "^2.13.5"
  }
}
